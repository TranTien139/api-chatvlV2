{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongo_db",
    "public": true
  },
  "AccessToken": {
    "dataSource": "mongo_db",
    "public": false
  },
  "ACL": {
    "dataSource": "mongo_db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo_db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo_db",
    "public": false
  },
  "Articles": {
    "dataSource": "mongo_db",
    "public": true
  },
  "Users": {
    "dataSource": "mongo_db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "register": true,
          "login": true,
          "logout": true
        }
      }
    }
  },
  "Comments": {
    "dataSource": "mongo_db",
    "public": true
  }
}
